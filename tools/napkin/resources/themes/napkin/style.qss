QWidget
{
    font-family: "Nunito Sans", sans-serif;
    font-size: 13px;
    color: #000;
    /*background-color: #3c3d4d;*/

    selection-background-color:#8B8CA0;
    selection-color: #111426;
    /*selection-color: #eff0f1;*/
    /*background-clip: border;*/
    /*border-image: none;*/
    /*border: 0px transparent black;*/
    /*outline: 0;*/

}
QMenuBar {
    color: #8B8CA0;
}

QMenu {
    background-color: #343544;
    color: #8B8CA0;
}

QDialog, QLabel {
    background-color: #2D2E42;
    color: #8B8CA0;
}

QAbstractScrollArea {
    color: #8B8CA0;
    background: #2D2E42;
    alternate-background-color: #303144;
    border: 0;
}

QHeaderView:section {
    color: #5D5E73;
    background-color: #252638;
    border: 0;
}

QGroupBox {
    border: 0;
    margin-top: 22px;
    background-color: #999;
}

QGroupBox:title {
    color: #444;
    background-color: #999;
}

QDockWidget {
    color: #b0b2cb;
    border: 1px solid #444;
    font-size: 12px;
    font-family: "Montserrat";

    /*padding: 10px;*/
}

QDockWidget::title {
    text-align: left;
    background: #5D5E73;
    padding: 4px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QLineEdit {
    border: none;
    border-radius: 2px;
    padding-left: 10px;
    padding-right: 10px;
    color: #8B8CA0;
    background: #1a1b2b;
}

QLineEdit:focus {
    color: #8B8CA0;
    background: #111426;
}

QScrollBar:horizontal,
QScrollBar:vertical {
    border-radius: 2px;
    background: #252638;
}

QScrollBar::handle:horizontal,
QScrollBar::handle:vertical {
    background: #343544;
    border-radius: 2px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
    background: #5D5E73;
    border-radius: 2px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal,
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QPushButton, QToolButton {
    font-family: "Nunito Sans ExtraBold";
    color: #2D2E42;
    background: #5D5E73;
    border-radius: 2px;
    margin: 4px;
    border: 0;
    padding: 3px 10px 3px 10px;
}

QPushButton:hover {
    background: #8B8CA0;
}
QPushButton:pressed {
    background: #111426;
    color: #8B8CA0
}
QPushButton:!enabled {
    background-color: #3c3d4d;
    color: #5D5E73;
}

QTabBar:tab {
    padding: 2px 6px 2px 6px;
    color: #b0b2cb;
    background: #5D5E73;
    border: 0;
}

QTabBar:tab::!selected {
    color: #8B8CA0;
    background: #45465a;
}

QStatusBar {
    color: #AAA;
    background: #555;
}

QGraphicsView {
    background: #555;
}

