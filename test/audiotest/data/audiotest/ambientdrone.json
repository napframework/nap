{
	"Objects":
	[
		{
			"Type": "nap::audio::AudioFileResource",
			"mID": "audioFile",
			"AudioFilePath": "data/audiotest/hang.mp3"
		},

		{
			"Type": "nap::audio::Graph",
			"mID": "graph",
			"Objects": [
				{
					"Type": "nap::audio::BufferPlayer",
					"mID": "bufferPlayer",
					"ChannelCount": 2,
					"Buffer": "audioFile",
					"autoPlay": "True"
				},
				{
					"Type": "nap::audio::Envelope",
					"mID": "envelope",
					"AutoTrigger": "True",
					"Envelope": [
						{
							"Duration": 6000,
							"Destination": 1
						},
						{
							"Duration": 1000,
							"Destination": 0.5
						},
						{
							"Duration": 1000,
							"Destination": 2
						},
						{
							"Duration": 6000,
							"Destination": 0.1
						}
					]
				},
				{
					"Type": "nap::audio::Oscillator",
					"mID": "oscillatorA",
					"ChannelCount": 2,
					"Frequency": [110, 200],
					"Amplitude": [1],
					"FmInput": "bufferPlayer"
				},
				{
					"Type": "nap::audio::Gain",
					"mID": "fmIndex",
					"ChannelCount": 2,
					"Inputs": ["oscillatorA", "envelope"],
					"Gain": [ 4 ]
				},
				{
					"Type": "nap::audio::Oscillator",
					"mID": "oscillatorB",
					"ChannelCount": 2,
					"Frequency": [330, 210],
					"Amplitude": [0.5],
					"FmInput": "fmIndex"
				}
			],
			"Output": "oscillatorB"
		},


		{
			"Type": "nap::Entity",
			"mID": "audioEntity",
			"Components":
			[
				{					
					"Type": "nap::audio::AudioComponent",
					"mID": "audioComponent",
					"Object":
					{
						"Type": "nap::audio::GraphObject",
						"Graph": "graph"
					}
				},

				{
					"Type": "nap::audio::OutputComponent",
					"mID": "output",
					"Routing": [ 0, 1 ],
					"Input": "audioComponent"
				}


			]
		}
	]
}