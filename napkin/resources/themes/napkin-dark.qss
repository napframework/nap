QWidget
{
    font-family: "Nunito Sans";
    font-size: 13px;
    color: #000;
    background-color: #111426;

    selection-background-color:#C86969;
    selection-color: #111426;
    /*selection-color: #eff0f1;*/
    /*background-clip: border;*/
    /*border-image: none;*/
    /*border: 0px transparent black;*/
    /*outline: 0;*/

}

QMenu, QDialog, QLabel {
    background-color: #2D2E42;
    color: #8B8CA0;
}

QAbstractScrollArea {
    color: #8B8CA0;
    background: #111426;
    border: 0;
}

QHeaderView:section {
    color: #5D5E73;
    background-color: #111426;
    border: 0;
}

QGroupBox {
    border: 0;
    margin-top: 22px;
    background-color: #999;
}

QGroupBox:title {
    color: #444;
    background-color: #999;
}

QDockWidget {
    color: #C86969;
    border: 1px solid #444;
    font-size: 16px;
    font-family: "Montserrat ExtraBold";

    /*padding: 10px;*/
}

QLineEdit {
    border: none;
    border-radius: 10px;
    padding-left: 10px;
    padding-right: 10px;
    color: #8B8CA0;
    background: #2D2E42;
}

QLineEdit:focus {
    color: #111426;
    background: #8B8CA0;
}

QDockWidget::title {
    text-align: left;
    background: #111426;
    padding: 1px;
    /*border-top-left-radius: 8px;*/
    /*border-top-right-radius: 8px;*/
}

QScrollBar:horizontal,
QScrollBar:vertical {
    border-radius: 7px;
    background: #111426;
}

QScrollBar::handle:horizontal,
QScrollBar::handle:vertical {
    background: #3c3d4d;
    border-radius: 7px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
    background: #C86969;
    border-radius: 7px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal,
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
}

QPushButton, QToolButton {
    font-family: "Nunito Sans ExtraBold";
    color: #2D2E42;
    background: #C86969;
    border-radius: 12px;
    border: 0;
    padding: 3px 10px 3px 10px;
}

QPushButton:hover {
    background: #444;
}
QPushButton:pressed {
    background: #222;
}
QPushButton:!enabled {
    background-color: #3c3d4d;
    color: #5D5E73;
}

QTabBar:tab {
    padding: 2px 6px 2px 6px;
    color: #DDD;
    background: #555;
    border: 0;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

QTabBar:tab::!selected {
    color: #222;
    background: #777;
}

QStatusBar {
    color: #AAA;
    background: #555;
}

QGraphicsView {
    background: #555;
}

